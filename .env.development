# Development Environment Variables
NODE_ENV=development
PORT=3000
NEXT_PUBLIC_API_URL=http://localhost:4000

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/ece_backend
REDIS_URL=redis://localhost:6379

# Authentication
JWT_SECRET=development-secret-key-change-in-production
SESSION_SECRET=development-session-secret

# External APIs (Mock for development)
CONTINUE_API_KEY=mock-continue-api-key
COPILOT_API_KEY=mock-copilot-api-key
VERCEL_TOKEN=mock-vercel-token

# Feature Flags
ENABLE_REAL_AI_AGENTS=false
ENABLE_MONITORING=true
ENABLE_DEBUG_MODE=true

# Monitoring
PROMETHEUS_ENABLED=false
GRAFANA_ENABLED=false

# Build Configuration
NEXT_TELEMETRY_DISABLED=1
ANALYZE_BUNDLE=false
